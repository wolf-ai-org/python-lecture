    "# 예제 1: 데이터 변환 파이프라인\n",
    "def data_pipeline(raw_data, transformations):\n",
    "    \"\"\"\n",
    "    데이터에 일련의 변환을 순차적으로 적용\n",
    "    \n",
    "    Args:\n",
    "        raw_data: 원시 데이터 리스트\n",
    "        transformations: 변환 함수들의 리스트\n",
    "    \n",
    "    Returns:\n",
    "        변환된 데이터\n",
    "    \"\"\"\n",
    "    result = raw_data\n",
    "    \n",
    "    for transform in transformations:\n",
    "        result = transform(result)\n",
    "    \n",
    "    return result\n",
    "\n",
    "# 변환 함수들 정의\n",
    "def remove_none_values(data):\n",
    "    return list(filter(lambda x: x is not None, data))\n",
    "\n",
    "def convert_to_numbers(data):\n",
    "    def safe_convert(item):\n",
    "        try:\n",
    "            return float(item)\n",
    "        except (ValueError, TypeError):\n",
    "            return None\n",
    "    return list(filter(lambda x: x is not None, map(safe_convert, data)))\n",
    "\n",
    "def apply_math_operation(operation):\n",
    "    return lambda data: list(map(operation, data))\n",
    "\n",
    "# 테스트\n",
    "messy_data = ['1', '2.5', None, '3', 'invalid', '4.2', '', '5']\n",
    "\n",
    "transformations = [\n",
    "    remove_none_values,\n",
    "    convert_to_numbers,\n",
    "    apply_math_operation(lambda x: x ** 2)  # 제곱\n",
    "]\n",
    "\n",
    "result = data_pipeline(messy_data, transformations)\n",
    "\n",
    "print(\"=== 데이터 변환 파이프라인 ===\")\n",
    "print(f\"원본 데이터: {messy_data}\")\n",
    "print(f\"변환 결과: {result}\")\n",
    "print()\n",
    "\n",
    "# 예제 2: 조건부 집계 시스템\n",
    "def conditional_aggregator(data, conditions, aggregation_func):\n",
    "    \"\"\"\n",
    "    조건을 만족하는 데이터만 선택해서 집계 수행\n",
    "    \n",
    "    Args:\n",
    "        data: 데이터 리스트\n",
    "        conditions: 조건 함수들의 리스트\n",
    "        aggregation_func: 집계 함수\n",
    "    \n",
    "    Returns:\n",
    "        집계 결과\n",
    "    \"\"\"\n",
    "    # 모든 조건을 만족하는 데이터만 필터링\n",
    "    filtered_data = list(filter(\n",
    "        lambda item: all(condition(item) for condition in conditions),\n",
    "        data\n",
    "    ))\n",
    "    \n",
    "    if not filtered_data:\n",
    "        return None\n",
    "    \n",
    "    return aggregation_func(filtered_data)\n",
    "\n",
    "# 학생 데이터\n",
    "students = [\n",
    "    {'name': '김철수', 'age': 20, 'score': 85, 'major': 'CS'},\n",
    "    {'name': '이영희', 'age': 22, 'score': 92, 'major': 'Math'},\n",
    "    {'name': '박민수', 'age': 21, 'score': 78, 'major': 'CS'},\n",
    "    {'name': '최지영', 'age': 20, 'score': 95, 'major': 'Physics'},\n",
    "    {'name': '정수호', 'age': 23, 'score': 88, 'major': 'CS'}\n",
    "]\n",
    "\n",
    "# 조건들 정의\n",
    "is_cs_major = lambda student: student['major'] == 'CS'\n",
    "is_adult = lambda student: student['age'] >= 21\n",
    "has_good_score = lambda student: student['score'] >= 80\n",
    "\n",
    "# 다양한 집계 수행\n",
    "print(\"=== 조건부 집계 시스템 ===\")\n",
    "\n",
    "# CS 전공이면서 성인인 학생들의 평균 점수\n",
    "avg_score = conditional_aggregator(\n",
    "    students,\n",
    "    [is_cs_major, is_adult],\n",
    "    lambda data: sum(s['score'] for s in data) / len(data)\n",
    ")\n",
    "print(f\"CS 전공 성인 학생들의 평균 점수: {avg_score:.1f}\")\n",
    "\n",
    "# 점수가 80점 이상인 학생 수\n",
    "good_students_count = conditional_aggregator(\n",
    "    students,\n",
    "    [has_good_score],\n",
    "    len\n",
    ")\n",
    "print(f\"80점 이상 학생 수: {good_students_count}명\")\n",
    "\n",
    "# 모든 조건을 만족하는 학생들의 이름\n",
    "top_cs_students = conditional_aggregator(\n",
    "    students,\n",
    "    [is_cs_major, is_adult, has_good_score],\n",
    "    lambda data: [s['name'] for s in data]\n",
    ")\n",
    "print(f\"CS 전공, 성인, 우수 학생들: {top_cs_students}\")\n",
    "print()\n",
    "\n",
    "# 예제 3: 객체 검증 시스템\n",
    "def validate_object(obj, validators):\n",
    "    \"\"\"\n",
    "    객체가 모든 검증 조건을 만족하는지 확인\n",
    "    \n",
    "    Args:\n",
    "        obj: 검증할 객체\n",
    "        validators: 검증 함수들의 딕셔너리\n",
    "    \n",
    "    Returns:\n",
    "        검증 결과 딕셔너리\n",
    "    \"\"\"\n",
    "    results = {}\n",
    "    \n",
    "    for name, validator in validators.items():\n",
    "        try:\n",
    "            results[name] = validator(obj)\n",
    "        except Exception as e:\n",
    "            results[name] = f\"Error: {str(e)}\"\n",
    "    \n",
    "    results['all_valid'] = all(\n",
    "        result is True for result in results.values() if result != \"Error\"\n",
    "    )\n",
    "    \n",
    "    return results\n",
    "\n",
    "# 검증 함수들 정의\n",
    "validators = {\n",
    "    'has_name': lambda obj: hasattr(obj, 'name') and isinstance(obj.name, str),\n",
    "    'has_age': lambda obj: hasattr(obj, 'age') and isinstance(obj.age, int),\n",
    "    'age_valid': lambda obj: 0 <= obj.age <= 150,\n",
    "    'name_not_empty': lambda obj: len(obj.name.strip()) > 0\n",
    "}\n",
    "\n",
    "# 테스트 객체들\n",
    "class Person:\n",
    "    def __init__(self, name, age):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "\n",
    "test_objects = [\n",
    "    Person(\"Alice\", 25),\n",
    "    Person(\"\", 30),\n",
    "    Person(\"Bob\", -5),\n",
    "    Person(\"Charlie\", 200)\n",
    "]\n",
    "\n",
    "print(\"=== 객체 검증 시스템 ===\")\n",
    "for i, obj in enumerate(test_objects):\n",
    "    validation_result = validate_object(obj, validators)\n",
    "    print(f\"객체 {i+1} (name='{obj.name}', age={obj.age}):\")\n",
    "    for key, value in validation_result.items():\n",
    "        print(f\"  {key}: {value}\")\n",
    "    print()"
   ]
