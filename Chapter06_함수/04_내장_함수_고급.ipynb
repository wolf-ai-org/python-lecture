    "    [False, False, False],\n",
    "    [1, 2, 3],\n",
    "    [0, 1, 2],\n",
    "    [0, 0, 0]\n",
    "]\n",
    "\n",
    "for case in test_cases:\n",
    "    print(f\"{case} → all(): {all(case)}, any(): {any(case)}\")\n",
    "\n",
    "# 실용적인 예제\n",
    "scores = [85, 90, 78, 92, 88]\n",
    "print(f\"\\n모든 점수가 60점 이상? {all(score >= 60 for score in scores)}\")\n",
    "print(f\"90점 이상인 점수가 있나? {any(score >= 90 for score in scores)}\")\n",
    "\n",
    "# 조건 검증 예제\n",
